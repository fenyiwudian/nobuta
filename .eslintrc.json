{
    "env": {
        "browser": true,
        "commonjs": true,
        "es6": true,
        "node": true,
        "jquery": true
    },
    "extends": "eslint:recommended",
    "parserOptions": {
        "sourceType": "module",
        "ecmaVersion": 8,
        "ecmaFeatures": {
          "experimentalObjectRestSpread": true
        }
      },
    "rules": {
        "no-console":0,
        "indent": [
            "error",
            2
        ],
        "semi": [
            "error",
            "always"
        ]
    }
}           