{
 "compilerOptions": {
    "outDir": "./build/",
    "noImplicitAny": true,
    "noImplicitThis": true,
    "noImplicitReturns": true,
    "module": "es2015",
    "target": "es2017",
    "allowSyntheticDefaultImports": true,
    "noUnusedLocals": true,
    "sourceMap": true,
    "lib": [
      "es2017",
      "dom",
      "dom.iterable"
    ]
  },
  "include": [
    "src/**/*.ts",
  ],
}